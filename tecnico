# Algoritmo "livros"

livros = int(input("digite a quantidade de livros:"))

if livros <= 1:
   print("permanece o valor")
elif 2 <= livros <= 3:
   print("desonto de 10% sobre o valor total")
elif 4 <= livros <= 5:
   print("desconto de 20% sobre o valor total")
elif livros >= 6:
   print("desconto de 30% sobre o valor")

